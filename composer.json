{
  "name": "tourze/lark-app-bot-bundle",
  "description": "A Symfony bundle for integrating Lark (Feishu) App Bot functionality",
  "type": "symfony-bundle",
  "keywords": [
    "lark",
    "feishu",
    "bot",
    "symfony",
    "bundle",
    "api",
    "webhook",
    "messaging"
  ],
  "homepage": "https://github.com/tourze/lark-app-bot-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Tourze Team",
      "email": "dev@tourze.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-mbstring": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "monolog/monolog": "^3.1",
    "promphp/prometheus_client_php": "^2.10",
    "psr/cache": "^2.0 || ^3.0",
    "psr/log": "^3|^2|^1",
    "symfony/cache": "^7.3",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/http-client-contracts": "^3.6",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/stopwatch": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-indexed-bundle": "1.0.*",
    "tourze/doctrine-timestamp-bundle": "1.0.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-routing-auto-loader-bundle": "1.0.*"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\LarkAppBotBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\LarkAppBotBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.9-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}